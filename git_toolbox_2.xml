<application>
  <component name="GitToolBoxAppSettings2">
    <option name="commitMessageIssueKeyValidation" value="true" />
    <option name="commitMessageValidationConfig">
      <CommitMessageValidationConfig>
        <option name="regexConfig">
          <RegexConfig>
            <option name="expression" value="\[[A-Z]{2}-\d+\] (fix|chore|docs|feat|refactor|style|test)(?:\(.*\))?: [a-zA-Z].*" />
            <option name="testInput" value="[AT-971] chore: find attendance method" />
          </RegexConfig>
        </option>
      </CommitMessageValidationConfig>
    </option>
    <option name="commitMessageValidationEnabled" value="true" />
    <option name="startupNotificationVersion" value="1" />
  </component>
</application>